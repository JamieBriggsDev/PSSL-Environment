<Application
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             xmlns:local="clr-namespace:PSSL_Environment"
             xmlns:DataStore="clr-namespace:Expression.Blend.DataStore.DataStore" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" x:Class="PSSL_Environment.App"
             Startup="Application_Startup">
    <Application.Resources>
        <DataStore:DataStore x:Key="DataStore" d:IsDataSource="True"/>
        <!--Title and main global app strings-->
        <sys:String x:Key="strAppName">PSSL Environment</sys:String>
        <!--Chose what colour you wish the pixel shader to be-->
        <sys:String x:Key="strSingleColour">Single Colour</sys:String>
        <sys:String x:Key="strTexture">Texture</sys:String>
        <sys:String x:Key="strColourTemplate">Template</sys:String>
    </Application.Resources>
</Application>
 