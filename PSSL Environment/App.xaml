<Application x:Class="PSSL_Environment.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             xmlns:local="clr-namespace:PSSL_Environment"
             Startup="Application_Startup">
    <Application.Resources>
        <!--Title and main global app strings-->
        <sys:String x:Key="strAppName">PSSL Environment</sys:String>
        <!--Chose what colour you wish the pixel shader to be-->
        <sys:String x:Key="strSingleColour">Single Colour</sys:String>
        <sys:String x:Key="strGradient">Gradient</sys:String>
        <sys:String x:Key="strColourTemplate">Template</sys:String>
    </Application.Resources>
</Application>
